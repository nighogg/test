int solution(int X, int A[], int N) {  
int i,j,k,l;    
int sum_l,sum_p;   
i=0;
    j=0;
    k=0;
    
for(k=0;k<=N;k++){       
sum_l=0;   
for(i=0;i<=k;i++)
    {       
if(A[i]==X) sum_l++;       
}       
sum_p=0;   
for(j=i+1;j<=N;j++)   
{if(A[j]!=X) {sum_p++;}
}    
if(sum_l==sum_p){return k+1;}   
}  
}
